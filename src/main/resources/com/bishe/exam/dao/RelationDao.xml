<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bishe.exam.dao.RelationDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bishe.exam.domain.Relation">
        <id column="id" property="id" />
        <result column="examId" property="examId" />
        <result column="studentId" property="studentId" />
        <result column="teacherId" property="teacherId" />
        <result column="classroomId" property="classroomId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, examId, studentId, teacherId, classroomId
    </sql>

</mapper>
