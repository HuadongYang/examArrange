<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bishe.exam.dao.ClassroomDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bishe.exam.domain.Classroom">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="area" property="area" />
        <result column="type" property="type" />
        <result column="floorNum" property="floorNum" />
        <result column="numTotal" property="numTotal" />
        <result column="numExam" property="numExam" />
        <result column="canClass" property="canClass" />
        <result column="canBorrow" property="canBorrow" />
        <result column="canExam" property="canExam" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, area, type, floorNum, numTotal, numExam, canClass, canBorrow, canExam
    </sql>

</mapper>
